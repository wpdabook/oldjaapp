<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_gtxt"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFF8FF"
    tools:context="com.a21zhewang.constructionapp.ui.repository.RepositoryActivity">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="80px">

        <com.a21zhewang.constructionapp.customview.MyTitleBar
            android:id="@+id/activity_index_MyTitleBar"
            android:layout_width="match_parent"
            android:layout_height="80px"
            app:backlin="true"
            app:righttext=""
            app:rightvisiable="true"
            app:titletext="视频监控" />

        <ImageView
            android:id="@+id/activity_index_img_sys"
            android:layout_width="80px"
            android:layout_height="80px"
            android:layout_gravity="right|center_vertical"
            android:clickable="true"
            android:src="@mipmap/addimgicowhite"
            android:visibility="gone" />

    </FrameLayout>
    <LinearLayout
        android:layout_marginTop="3dp"
        android:layout_height="match_parent"
        android:layout_width="match_parent" >
        <WebView
            android:id="@+id/video_webview"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            />
    </LinearLayout>
</LinearLayout>
